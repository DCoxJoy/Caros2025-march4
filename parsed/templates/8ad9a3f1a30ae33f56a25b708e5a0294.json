{"components/common/cookie":"<div class=\"cookieMessage\">\n    <a href=\"#\" class=\"close\" data-close-cookieMessage><span aria-hidden=\"true\">&#215;</span></a>\n    <div class=\"cookieMessage-container\">\n        <div class=\"cookieMessage-text\">\n            <p>{{settings.privacy_cookie}}</p>\n        </div>\n        <div class=\"group-button\">\n            <a class=\"privacy_accept\" href=\"javascript:void(0)\" data-privacy-accept>{{lang 'cookies.accept'}}</a>\n            <a class=\"privacy_policy\" href=\"{{theme_settings.privacy_policy_url}}\">{{theme_settings.privacy_policy}}</a>\n        </div>\n    </div>\n</div>\n","components/common/footer":"{{#if theme_settings.themevale_homepage_layout '!==' 'homepage layout 3'}}\n    <div class=\"themevale_service\">\n        <div class=\"container\">\n            <div class=\"themevale_column themevale_column--col4 themevale_customColumn\">\n                <div class=\"item\">\n                    <div class=\"service-wrapper\">\n                        <div class=\"icon\">\n                            <svg><use xlink:href=\"#{{theme_settings.themevale_customer-service-icon-1}}\"></use></svg>\n                        </div>\n                        <div class=\"title\">{{theme_settings.themevale_customer-service-title-1}}</div>\n                    </div>\n                </div>\n                <div class=\"item\">\n                    <div class=\"service-wrapper\">\n                        <div class=\"icon\">\n                            <svg><use xlink:href=\"#{{theme_settings.themevale_customer-service-icon-2}}\"></use></svg>\n                        </div>\n                        <div class=\"title\">{{theme_settings.themevale_customer-service-title-2}}</div>\n                    </div>\n                </div>\n                <div class=\"item\">\n                    <div class=\"service-wrapper\">\n                        <div class=\"icon\">\n                            <svg><use xlink:href=\"#{{theme_settings.themevale_customer-service-icon-3}}\"></use></svg>\n                        </div>\n                        <div class=\"title\">{{theme_settings.themevale_customer-service-title-3}}</div>\n                    </div>\n                </div>\n                <div class=\"item\">\n                    <div class=\"service-wrapper\">\n                        <div class=\"icon\">\n                            <svg><use xlink:href=\"#{{theme_settings.themevale_customer-service-icon-4}}\"></use></svg>\n                        </div>\n                        <div class=\"title\">{{theme_settings.themevale_customer-service-title-4}}</div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n{{/if}}\n<footer class=\"footer\">\n    <div class=\"container\">\n        <div class=\"themevale_topFooter\">\n            <h4 class=\"text\">{{theme_settings.footer-top-content}}</h4>\n            <a class=\"phone\" href=\"tel:{{theme_settings.footer-top-button}}\">{{theme_settings.footer-top-button}}</a>\n        </div>\n        <div class=\"themevale_middleFooter\">\n            <div class=\"footer-info themevale_footer-info col-5\">\n                <div class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\n                    <div class=\"item\">\n                        {{#if settings.show_newsletter_box}}\n                            {{> components/common/subscription-form}}\n                        {{/if}}\n                    </div>\n                    <div class=\"item\">\n                        {{> components/common/social-links}}\n                    </div>\n                </div>\n                <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\" data-section-type=\"footer-categories\">\n                    <h5 class=\"footer-info-heading\">\n                        <span>{{lang 'footer.shop'}}</span>\n                        <svg class=\"icon-dropdown\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n                    </h5>\n                    <ul class=\"footer-info-list\">\n                        {{#each (limit categories 9)}}\n                            <li>\n                                <a href=\"{{url}}\">{{name}}</a>\n                            </li>\n                        {{/each}}\n                    </ul>\n                </div>\n                <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\" data-section-type=\"footer-webPages\">\n                    <h5 class=\"footer-info-heading\">\n                        <span>{{lang 'footer.info'}}</span>\n                        <svg class=\"icon-dropdown\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n                    </h5>\n                    <ul class=\"footer-info-list\">\n                        {{#each (limit pages 9)}}\n                            <li>\n                                <a href=\"{{url}}\">{{name}}</a>\n                            </li>\n                        {{/each}}\n                    </ul>\n                </div>\n                <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\">\n                    <h5 class=\"footer-info-heading\">\n                        <span>{{lang 'footer.contact'}}</span>\n                        <svg class=\"icon-dropdown\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n                    </h5>\n                    <ul class=\"footer-info-list\">\n                        <li class=\"store-address\"><svg class=\"icon\"><use xlink:href=\"#icon-location-2\"></use></svg>{{nl2br settings.address}}</li>\n                        <li class=\"store-phone\"><svg class=\"icon\"><use xlink:href=\"#icon-telephone-2\"></use></svg><p>{{theme_settings.footer-middle-phone1}}</p><p>{{theme_settings.footer-middle-phone2}}</p><a class=\"ask_an_expert\" href=\"#\" data-reveal-id=\"ask-an-expert\">{{lang 'footer.ask_an_expert'}}</a></li>\n                        <li class=\"store-email\"><svg class=\"icon\"><use xlink:href=\"#icon-envelope-2\"></use></svg><a href=\"mailto:{{theme_settings.footer-middle-email}}\">Email: {{theme_settings.footer-middle-email}}</a></li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"footer-info-custom\">\n                <div class=\"item\">\n                    <span><i>Please note: All manufacturer names, symbols, and descriptions, used in our images & text are used solely for identification purposes only. It is neither inferred nor implied that any item sold by Speedo Demo is a product authorized by or in any way connected with any vehicle manufacturers displayed on this page.</i></span>\n                </div>\n                <div class=\"item\">\n                    <div class=\"themevale_securityImage\">\n                        <ul>\n                            <li>\n                                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.themevale_footer-security-image-1}}\" alt=\"lazyload\">\n                            </li>\n                            <li>\n                                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.themevale_footer-security-image-2}}\" alt=\"lazyload\">\n                            </li>\n                            <li>\n                                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.themevale_footer-security-image-3}}\" alt=\"lazyload\">\n                            </li>\n                            <li>\n                                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.themevale_footer-security-image-4}}\" alt=\"lazyload\">\n                            </li>\n                        </ul>\n                    </div>\n                    {{#if theme_settings.geotrust_ssl_common_name}}\n                        <div class=\"footer-geotrust-ssl-seal\">\n                            {{> components/common/geotrust-ssl-seal}}\n                        </div>\n                    {{/if}}\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"themevale_bottomFooter\">\n        <div class=\"container\">\n            <div class=\"item\">\n                {{#if theme_settings.show_copyright_footer}}\n                    <div class=\"footer-copyright\">\n                        <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\n                    </div>\n                {{/if}}\n                {{#if theme_settings.show_powered_by}}\n                    <div class=\"footer-copyright\">\n                        <p class=\"powered-by\">{{theme_settings.powered_by_content}}</p>\n                    </div>\n                {{/if}}\n            </div>\n            <div class=\"item\">\n                {{> components/common/payment-icons}}\n            </div>\n        </div>\n    </div>\n</footer>\n{{#if theme_settings.themevale_BackTop}}\n    <div id=\"back-to-top\">\n        <a href=\"javascript:void(0)\"><svg class=\"icon\"><use xlink:href=\"#icon-keyboard-arrow-down\"></use></svg></a>\n    </div>\n{{/if}}\n\n\n{{#if theme_settings.themevale_ask-an-expert}}\n    {{> components/themevale/ask-an-expert}}\n{{/if}}\n\n{{!-- {{#if theme_settings.themevale_RecentlyBought}}\n<div class=\"themevale_popup_left\"></div>\n{{/if}} --}}    \n\n{{#if settings.privacy_cookie }}\n    <div class=\"themevale_popup_right\">\n        {{> components/common/cookie }}\n    </div>\n{{/if}}\n\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n","components/common/payment-icons":"{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_sage show_accept_wallet show_accept_sage}}\n        <div class=\"footer-payment-image\">\n            {{#if show_accept_visa}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/payment-1.png\" alt=\"payment-1\">\n            {{/if}}\n            {{#if show_accept_mastercard}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/payment-2.png\" alt=\"payment-2\">\n            {{/if}}\n            {{#if show_accept_amex}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/payment-3.png\" alt=\"payment-3\">\n            {{/if}}\n            {{#if show_accept_discover}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/payment-4.png\" alt=\"payment-4\">\n            {{/if}}\n            {{#if show_accept_paypal}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/payment-5.png\" alt=\"payment-5\">\n            {{/if}}\n            {{#if show_accept_sage}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/payment-6.png\" alt=\"payment-6\">\n            {{/if}}\n            {{#if show_accept_wallet}}\n                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/payment-7.png\" alt=\"payment-7\">\n            {{/if}}\n        </div>\n    {{/or}}\n{{/with}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n","components/themevale/ask-an-expert":"<!-- BEGIN ASK AN EXPERT MODAL -->\r\n<div id=\"ask-an-expert\" class=\"modal\" data-reveal>\r\n    <div class=\"modal-content\">\r\n        <div class=\"modal-header\">\r\n            {{#if theme_settings.halo_ask_an_expert_title}}\r\n                <h2 class=\"modal-header-title\">{{theme_settings.halo_ask_an_expert_title}}</h2>\r\n            {{/if}}\r\n            <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n                <span aria-hidden=\"true\">&#215;</span>\r\n            </a>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n            <div id=\"contact_results\"></div>\r\n            <div class=\"form-inner\">\r\n                {{#if theme_settings.halo_ask_an_expert_desc1}}\r\n                    <div class=\"description\">{{theme_settings.halo_ask_an_expert_desc1}}</div>\r\n                {{/if}}\r\n                {{#if theme_settings.halo_ask_an_expert_desc2}}\r\n                    <div class=\"question\">{{theme_settings.halo_ask_an_expert_desc2}}</div>\r\n                {{/if}}\r\n                <div id=\"halo-ask-an-expert-results\" class=\"halo-ask-an-expert-text\"></div>\r\n                <form data-contact-form class=\"halo-ask-an-expert-form\" action=\"{{urls.contact_us_submit}}\"\r\n                    method=\"post\" data-product-id=\"{{product.id}}\" data-product-title=\"{{product.title}}\"\r\n                    data-product-url=\"{{product.url}}\" data-product-sku=\"{{product.sku}}\">\r\n                    <fieldset class=\"form-fieldset\">\r\n                        <div class=\"hl-form-field-wrapper\"></div>\r\n                        <div class=\"form-field form-field-radio\">\r\n                            <label class=\"form-label\">Do you need:</label>\r\n                            <input class=\"form-radio\" id=\"pricing-radio\" type=\"radio\" name=\"type_package\" checked\r\n                                value=\"Pricing\">\r\n                            <label class=\"form-label\" for=\"pricing-radio\">Pricing</label>\r\n                            <input class=\"form-radio\" id=\"answers-radio\" type=\"radio\" name=\"type_package\"\r\n                                value=\"Answers\">\r\n                            <label class=\"form-label\" for=\"answers-radio\">Answers</label>\r\n                            <input class=\"form-radio\" id=\"both-radio\" type=\"radio\" name=\"type_package\"\r\n                                value=\"Pricing & Answers\">\r\n                            <label class=\"form-label\" for=\"both-radio\">Both</label>\r\n                        </div>\r\n                        <div class=\"form-field\">\r\n                            <label class=\"form-label\" for=\"contact_comment_area\">What can i help you with today?\r\n                                <small>{{lang 'common.required' }}</small>\r\n                            </label>\r\n                            <textarea name=\"contact_comment_area\" cols=\"30\" rows=\"3\" class=\"form-input\"\r\n                                id=\"contact_comment_area\" required></textarea>\r\n                        </div>\r\n                        <div class=\"form-field form-field-radio form-field-radio2\">\r\n                            <label class=\"form-label form-label-custom\">How would you like me to contact you?</label>\r\n                            <input class=\"form-radio\" id=\"phone-radio\" type=\"radio\" name=\"type_contact\" checked\r\n                                value=\"Phone\">\r\n                            <label class=\"form-label\" for=\"phone-radio\">Phone</label>\r\n                            <input class=\"form-radio\" id=\"email-radio\" type=\"radio\" name=\"type_contact\" value=\"Email\">\r\n                            <label class=\"form-label\" for=\"email-radio\">Email</label>\r\n                            <input class=\"form-radio\" id=\"both-radio-2\" type=\"radio\" name=\"type_contact\"\r\n                                value=\"Phone & Email\">\r\n                            <label class=\"form-label\" for=\"both-radio-2\">Both</label>\r\n                        </div>\r\n                        <div class=\"hl-form-field\" style=\"display: none;\">\r\n                            <textarea name=\"contact_question\" cols=\"30\" rows=\"3\" class=\"form-input\"\r\n                                id=\"contact_question\"></textarea>\r\n                        </div>\r\n                        <div class=\"g-recaptcha\" data-sitekey=\"6LcjX0sbAAAAACp92-MNpx66FT4pbIWh-FTDmkkz\"></div>\r\n                        <div class=\"form-actions\">\r\n                            <button type=\"submit\" class=\"button button--primary\" id=\"halo-ask-an-expert-button\">Send\r\n                                Question</button>\r\n                        </div>\r\n                    </fieldset>\r\n                </form>\r\n                <script src=\"https://www.google.com/recaptcha/api.js\" async=\"\" defer=\"\"></script>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\n"}
