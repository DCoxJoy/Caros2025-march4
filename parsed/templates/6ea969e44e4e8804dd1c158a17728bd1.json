div class=\"themevale_header-middle\">\r\n            <div class=\"container\">\r\n                <div class=\"headerMiddle-item text-left\">\r\n                    <div class=\"item--hamburger\">\r\n                        <a class=\"mobileMenu-toggle\" href=\"#\" data-mobile-menu-toggle=\"menu\">\r\n                            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"logo-wrapper\">\r\n                        {{#if page_type '===' 'default'}}\r\n                            <h1 class=\"header-logo\">\r\n                                {{> components/common/store-logo}}\r\n                            </h1>\r\n                        {{else}}\r\n                            <div class=\"header-logo\">\r\n                                {{> components/common/store-logo}}\r\n                            </div>\r\n                        {{/if}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"headerMiddle-item text-right\">\r\n                    <div class=\"navUser-item navUser-item--quickSearch item--quickSearch\">\r\n                        <div class=\"themevale_search-custom layout-2\" id=\"quickSearch2\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n                            {{> components/themevale/header/quick-search-all}}\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"navUser-item navUser-item--account2\">\r\n                        <div class=\"navUser-action custom-2\">\r\n                            <span class=\"navUser-icon\">\r\n                                <svg class=\"icon\"><use xlink:href=\"#icon-user\"></use></svg>\r\n                            </span>\r\n                            <div class=\"navUser-text-wrapper\">\r\n                                {{#if customer}}\r\n                                    <a class=\"navUser-action navUser-action--myaccount\" href=\"{{urls.account.index}}\">\r\n                                        <span>{{lang 'common.account'}}</span>\r\n                                        <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                                    </a>\r\n                                    <ul class=\"dropdown-menu\" id=\"navPages-account-topbar\">\r\n                                        <li class=\"dropdown-menu-item\">\r\n                                            <a href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n                                        </li>\r\n                                        {{#if settings.returns_enabled}}\r\n                                            <li class=\"dropdown-menu-item\">\r\n                                                <a href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\r\n                                            </li>\r\n                                        {{/if}}\r\n                                        <li class=\"dropdown-menu-item\">\r\n                                            <a href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\r\n                                        </li>\r\n                                        <li class=\"dropdown-menu-item\">\r\n                                            <a href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\r\n                                        </li>\r\n                                        {{#if settings.show_payment_methods}}\r\n                                            <li class=\"dropdown-menu-item\">\r\n                                                <a href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\r\n                                            </li>\r\n                                        {{/if}}\r\n                                        <li class=\"dropdown-menu-item\">\r\n                                            <a href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\r\n                                        </li>\r\n                                        <li class=\"dropdown-menu-item\">\r\n                                            <a href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\r\n                                        </li>\r\n                                        <li class=\"dropdown-menu-item\">\r\n                                            <a href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\r\n                                        </li>\r\n                                        <li class=\"dropdown-menu-item\">\r\n                                            <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n                                        </li>\r\n                                    </ul>\r\n                                {{else}}\r\n                                    <a class=\"navUser-action navUser-action-loginLabel\" href=\"javascript:void(0)\" data-login-form>{{lang 'common.account'}}</a>\r\n                                    {{#if page_type '!==' 'login'}}\r\n                                        {{> components/themevale/header/login-form}}\r\n                                    {{/if}}\r\n                                {{/if}}\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"navUser-item navUser-item--wishList2\">\r\n                        <a class=\"navUser-action custom-2\" href=\"{{urls.account.wishlists.all}}\">\r\n                            <span class=\"navUser-icon\">\r\n                                <svg class=\"icon\"><use xlink:href=\"#icon-heart\"></use></svg>\r\n                            </span>\r\n                            <span class=\"navUser-text-wrapper\">\r\n                                <span class=\"navUser-text navUser-item-wishListLabel\">{{lang 'common.wishlist'}}</span>\r\n                            </span>\r\n                        </a>\r\n                    </div>\r\n                    <div class=\"navUser-item navUser-item--cart themevale_cart\">\r\n                        <a class=\"navUser-action custom-2\"\r\n                            data-cart-preview\r\n                            data-options=\"align:right\"\r\n                            href=\"{{urls.cart}}\">\r\n                            <span class=\"navUser-icon navUser-item-cartIcon2\">\r\n                                <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                                <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n                            </span>\r\n                            <span class=\"navUser-text-wrapper\">\r\n                                <span class=\"navUser-text navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\r\n                            </span>\r\n                        </a>\r\n                        <div class=\"dropdown-cart\" id=\"cart-preview-dropdown\" aria-hidden=\"true\"></div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"themevale_header-bottom\">\r\n            {{> components/themevale/header/navigation-menu-3}}\r\n        </div>\r\n    </div>\r\n    <div class=\"themevale_header-Mobile\">\r\n        {{> components/themevale/header/header-mobile}}\r\n    </div>\r\n</header>\n","components/themevale/header/header-mobile":"<div class=\"header-Mobile-item text-left\">\r\n    <div class=\"items item--hamburger\">\r\n    </div>\r\n    <div class=\"items item--searchMobile\">\r\n        <a class=\"navUser-action\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\r\n            <div class=\"navUser-icon\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-search2\"></use></svg>\r\n                <svg class=\"icon icon-close\"><use xlink:href=\"#times\"></use></svg>\r\n            </div>\r\n        </a>\r\n    </div>\r\n</div>\r\n<div class=\"header-Mobile-item text-center\">\r\n    <div class=\"items\">\r\n        <div class=\"logo-wrapper\">\r\n            {{#if page_type '===' 'default'}}\r\n                <h1 class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </h1>\r\n            {{else}}\r\n                <div class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</div>\r\n<div class=\"header-Mobile-item text-right\">\r\n    <div class=\"items item--account\">\r\n        <a class=\"navUser-action {{#if page_type '!==' 'login'}}accountMobile{{/if}}\" href=\"javascript:void(0)\">\r\n            <div class=\"navUser-icon\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-user\"/></svg>\r\n            </div>\r\n        </a>\r\n    </div>\r\n    <div class=\"items item--cart themevale_cart\">\r\n        <a class=\"navUser-action cartMobile\"\r\n            data-cart-preview\r\n            href=\"{{urls.cart}}\">\r\n            <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                <svg class=\"icon\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n            </div>\r\n        </a>\r\n    </div>\r\n</div>\r\n","components/themevale/header/login-form":"<div class=\"login-dropdown\" id=\"login-dropdown\">\r\n    <div class=\"login-header\">\r\n        <h2 class=\"login-title\">{{lang 'login.new_customer.heading2' }}</h2>\r\n        <a href=\"#\" class=\"close\" data-close-login-dropdown><span aria-hidden=\"true\">&#215;</span></a>\r\n    </div>\r\n    <div class=\"login-content\">\r\n        <form class=\"login-form form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\r\n            {{inject 'useValidEmail' (lang 'forms.validate.common.email_address')}}\r\n            {{inject 'enterPass' (lang 'forms.validate.common.password')}}\r\n            <p class=\"intro\">{{lang 'login.intro'}}</p>\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"login_email2\">{{lang 'login.field_email'}}</label>\r\n                <input class=\"form-input\" name=\"login_email\" id=\"login_email2\" type=\"email\">\r\n            </div>\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"login_pass2\">{{lang 'login.field_password'}}</label>\r\n                <input class=\"form-input\" id=\"login_pass2\" type=\"password\" name=\"login_pass\">\r\n            </div>\r\n            <div class=\"form-actions\">\r\n                <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\r\n                <input type=\"submit\" class=\"button button--border\" value=\"{{lang 'login.button' }}\">\r\n            </div>\r\n        </form>\r\n    </div>\r\n    {{#if settings.account_creation_enabled}}\r\n        <div class=\"login-footer\">\r\n            <p class=\"intro\">{{lang 'create_account.intro' }}</p>\r\n            <a class=\"button button--secondary\" href=\"{{urls.auth.create_account}}\">{{lang 'create_account.button' }}</a>\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n","components/themevale/header/navigation-3":"<div class=\"container\">\r\n    <div class=\"navUser text-left\">\r\n        <div class=\"navUser-item navUser-item--phone\">\r\n            <svg class=\"icon big\"><use xlink:href=\"#icon-telephone\"></use></svg>\r\n            <a class=\"navUser-action store-phone\" href=\"tel:{{settings.phone_number}}\">{{settings.phone_number}}</a>\r\n        </div>\r\n        <div class=\"navUser-item navUser-item--mail\">\r\n            <svg class=\"icon big\"><use xlink:href=\"#icon-envelope-3\"></use></svg>\r\n            <a class=\"navUser-action\" href=\"mailto:{{theme_settings.themevale_office-mail}}\">{{theme_settings.themevale_office-mail}}</a>\r\n        </div>\r\n    </div>\r\n    <div class=\"navUser text-center\">\r\n        <div class=\"navUser-item\"><p class=\"own-text\">{{theme_settings.themevale_office-text}}</p></div>\r\n    </div>\r\n    <div class=\"navUser text-right\">\r\n        <div class=\"navUser-item navUser-item--contact\">\r\n            <a class=\"navUser-action\" href=\"/contact-us/\">{{lang 'common.need_help'}}</a>\r\n        </div>\r\n        <div class=\"navUser-item\">\r\n            <a class=\"navUser-action\" href=\"/blogs/\">{{lang 'common.blog'}}</a>\r\n        </div>\r\n        {{#if settings.gift_certificates_enabled}}\r\n        <div class=\"navUser-item\">\r\n            <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\r\n        </div>\r\n        {{/if}}\r\n        <div class=\"navUser-item\">\r\n            <a class=\"navUser-action\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} (<span class=\"countPill\">0</span>)</a>\r\n        </div>\r\n        <div class=\"navUser-item navUser-item--currencySelector\">\r\n            {{> components/common/currency-selector}}\r\n        </div>\r\n    </div>\r\n</div>\r\n","components/themevale/header/navigation-menu-3":"<div class=\"navPages-container\" id=\"menu\" data-menu>\r\n    <div class=\"themevale_allCategories {{#if theme_settings.themevale_allCategories_open}}is-open{{/if}}\">\r\n        <p class=\"themevale_allCategories-text\">\r\n            <svg class=\"icon\"><use xlink:href=\"#menu-mobile\"></use></svg>\r\n            <span class=\"text\">{{lang 'category.label'}}</span>\r\n        </p>\r\n        <div class=\"themevale_allCategories-dropdown\">\r\n            <ul class=\"navPages-list\">\r\n                {{#each categories}}\r\n                    {{#or (if id '==' ../theme_settings.themevale_category-custom-1) (if id '==' ../theme_settings.themevale_category-custom-2) (if id '==' ../theme_settings.themevale_category-custom-3) (if id '==' ../theme_settings.themevale_category-custom-4) (if id '==' ../theme_settings.themevale_category-custom-5) (if id '==' ../theme_settings.themevale_category-custom-6)}}\r\n                    {{else}}\r\n                        <li class=\"navPages-item\">\r\n                            {{#if children}}\r\n                                <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\r\n                                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\r\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                    </span>\r\n                                </p>\r\n                                {{> components/common/navigation-dropdown}}\r\n                            {{else}}\r\n                                <a class=\"navPages-action\" href=\"{{url}}\">\r\n                                    <span class=\"text\">{{name}}</span>\r\n                                </a>\r\n                            {{/if}}\r\n                        </li>\r\n                    {{/or}}\r\n                {{/each}}\r\n            </ul>\r\n        </div>\r\n    </div>\r\n    <nav class=\"navPages\">\r\n        <ul class=\"navPages-list navPages-list--user\">\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"{{urls.home}}\"><span class=\"text\">{{lang 'home.heading'}}</span></a>\r\n            </li>\r\n            {{#each pages}}\r\n                <li class=\"navPages-item\">\r\n                    {{> components/common/navigation-list-alternate}}\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </nav>\r\n    {{#if theme_settings.themevale_header-layout-3-customlink}}\r\n    <div class=\"custom-link\">\r\n        <a class=\"navPages-action\" href=\"{{theme_settings.themevale_header-layout-3-customlink}}\"><span class=\"text\">{{theme_settings.themevale_header-layout-3-customtext}}</span></a>\r\n    </div>\r\n    {{/if}}\r\n</div>\n","components/themevale/header/quick-search-all":"{{{ snippet \"forms_search\"}}}\r\n<form class=\"form\" action=\"{{urls.search}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            <div id=\"search_category\">\r\n                <select name=\"category\" class=\"select_category\">\r\n                    <option value=\"\" selected=\"\">All</option>\r\n                    {{#each categories}}\r\n                        <option value=\"{{id}}\">{{name}}</option>\r\n                        {{#if children}}\r\n                            {{#each children}}\r\n                                <option value=\"{{id}}\">&ndash;&ndash;&ndash; {{name}}</option>\r\n                                {{#if children}}\r\n                                    {{#each children}}\r\n                                        <option value=\"{{id}}\">&ndash;&ndash;&ndash;&ndash;&ndash;&ndash; {{name}}</option>\r\n                                        {{#if children}}\r\n                                            {{#each children}}\r\n                                                {{#if children}}\r\n                                                    <option value=\"{{id}}\">&ndash;&ndash;&ndash;&ndash;&ndash;&ndash;&ndash;&ndash;&ndash; {{name}}</option>\r\n                                                {{else}}\r\n                                                    <option value=\"{{id}}\">&ndash;&ndash;&ndash;&ndash;&ndash;&ndash;&ndash;&ndash;&ndash; {{name}}</option>\r\n                                                {{/if}}\r\n                                            {{/each}}\r\n                                        {{/if}}\r\n                                    {{/each}}\r\n                                {{/if}}\r\n                            {{/each}}\r\n                        {{/if}}\r\n                    {{/each}}\r\n                </select> \r\n                <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n            </div>\r\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_quickSearch'}}\" autocomplete=\"off\">\r\n            <button class=\"btn-search\" type=\"submit\"><svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg><span>Search</span></button>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n<div class=\"quickSearchResults\" data-bind=\"html: results\"></div>\r\n"}
