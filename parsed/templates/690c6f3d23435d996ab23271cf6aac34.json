{"components/faceted-search/facets/hierarchy":"{{#if items}}\n<div class=\"accordion-block sidebarBlock\">\n    <div\n        class=\"accordion-navigation toggleLink is-open\"\n        role=\"button\"\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\n        <h5 class=\"sidebarBlock-heading\">\n             {{{title}}}\n        </h5>\n\n        <div class=\"accordion-navigation-actions\">\n            {{#if selected}}\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\n            {{/if}}\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use xlink:href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use xlink:href=\"#icon-remove\" />\n            </svg>\n        </div>\n    </div>\n\n    <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"sidebarBlock-content accordion-content is-open\">\n        <ul id=\"facetedSearch-navList--{{hyphenate facet }}\" class=\"navList\" data-facet=\"{{facet}}\" data-has-more-results=\"{{ has_more_results }}\">\n            {{#each items}}\n                <li class=\"navList-item\">\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\n                        rel=\"nofollow\"\n                        data-id=\"{{ id }}\"\n                        data-faceted-search-facet>\n                        {{{title}}}\n                        {{#if ../show_product_counts}}\n                            {{#if count}}\n                                <span>({{ count }})</span>\n                            {{/if}}\n                        {{/if}}\n                    </a>\n                    {{#if children}}\n                        {{> components/faceted-search/facets/hierarchy-children children}}\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n\n        {{#if show_more_toggle}}\n            <a href=\"#facetedSearch-navList--{{hyphenate facet }}\" role=\"button\" class=\"toggleLink\">\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\n            </a>\n        {{/if}}\n    </div>\n</div>\n{{/if}}\n","components/faceted-search/facets/hierarchy-children":"<ul class=\"navList navList--inner\">\n    {{#each this}}\n        <li class=\"navList-item\">\n            <a\n            href=\"{{url}}\"\n            class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\n            rel=\"nofollow\"\n            data-id=\"{{ id }}\"\n            data-faceted-search-facet>\n                {{{title}}}\n                {{#if ../show_product_counts}}\n                    {{#if count}}\n                        <span>({{count}})</span>\n                    {{/if}}\n                {{/if}}\n            </a>\n            {{#if children}}\n                {{> components/faceted-search/facets/hierarchy-children children}}\n            {{/if}}\n        </li>\n    {{/each}}\n</ul>\n","components/faceted-search/facets/multi":"{{#if items}}\n    {{#or ( if title '==' 'Brand') ( if title '==' 'Device Type' ) ( if title '==' 'Industry')}} \n    <div class=\"accordion-block sidebarBlock\">\n        <div\n            class=\"accordion-navigation toggleLink\"\n            role=\"button\"\n            data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\"\n            aria-controls=\"facetedSearch-content--{{hyphenate facet }}\"\n            aria-expanded=\"false\">\n            <h5 class=\"sidebarBlock-heading\">\n                {{{title}}}\n            </h5>\n\n            <div class=\"accordion-navigation-actions\">\n                {{#if selected}}\n                    <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\n                {{/if}}\n                <img src=\"{{cdn 'assets/img/open.svg'}}\" class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\" alt=\"Open Icon\" />\n                <img src=\"{{cdn 'assets/img/close.svg'}}\" class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\" alt=\"Close Icon\" />\n\n            </div>\n        </div>\n\n        <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"sidebarBlock-content accordion-content\" aria-hidden=\"true\">\n            <ul id=\"facetedSearch-navList--{{hyphenate facet }}\" data-facet=\"{{facet}}\" class=\"navList\" data-has-more-results=\"{{ has_more_results }}\">\n                {{#each items}}\n                <li class=\"navList-item\">\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}}\"\n                        rel=\"nofollow\"\n                        data-faceted-search-facet>\n                        {{{title}}}\n                        {{#if ../show_product_counts}}\n                            <span>({{ count }})</span>\n                        {{/if}}\n\n                        <span class=\"navList-action-close\" aria-hidden=\"true\">\n                            <svg class=\"icon\">\n                                <use xlink:href=\"#icon-close\"/>\n                            </svg>\n                        </span>\n                    </a>\n                </li>\n                {{/each}}\n            </ul>\n\n            {{#if show_more_toggle}}\n                <a href=\"#facetedSearch-navList--{{hyphenate facet }}\" role=\"button\" class=\"toggleLink\">\n                    <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\n                </a>\n            {{/if}}\n        </div>\n    </div>\n    {{/or}}\n{{/if}}\n","components/faceted-search/facets/range":"<!-- <div class=\"accordion-block sidebarBlock comment\">\n    <div\n        class=\"accordion-navigation toggleLink is-open\"\n        role=\"button\"\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\n        <h5 class=\"sidebarBlock-heading\">\n            {{{title}}}\n        </h5>\n\n        <div class=\"accordion-navigation-actions\">\n            {{#if selected}}\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\n            {{/if}}\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use xlink:href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use xlink:href=\"#icon-remove\" />\n            </svg>\n        </div>\n    </div>\n\n    <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"sidebarBlock-content accordion-content is-open\">\n        <form id=\"facet-range-form\" class=\"form\" method=\"get\" data-faceted-search-range novalidate>\n            {{#each current_selected_items}}\n                <input type=\"hidden\" name=\"{{ param_name }}[]\" value=\"{{ param_value }}\"/>\n            {{/each}}\n            <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\n            {{#if this.sort}}\n                <input type=\"hidden\" name=\"sort\" value={{this.sort}}>\n            {{/if}}\n            <fieldset class=\"form-fieldset\">\n                <div class=\"form-minMaxRow\">\n                    <div class=\"form-field\">\n                        <input\n                            name=\"min_price\"\n                            placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{ min_price }}\"\n                        />\n                    </div>\n\n                    <div class=\"form-field\">\n                        <input\n                            name=\"max_price\"\n                            placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\n                            min=\"0\"\n                            class=\"form-input form-input--small\"\n                            required\n                            type=\"number\"\n                            value=\"{{ max_price }}\"\n                        />\n                    </div>\n\n                    <div class=\"form-field\">\n                        <button class=\"button button--small\" type=\"submit\">\n                            {{lang 'search.faceted.range.update'}}\n                        </button>\n                    </div>\n                </div>\n\n                <div class=\"form-inlineMessage\"></div>\n            </fieldset>\n        </form>\n    </div>\n</div> -->\n","components/faceted-search/facets/rating":"{{#if items}}\n<div class=\"accordion-block sidebarBlock comment\">\n    <div\n        class=\"accordion-navigation toggleLink is-open\"\n        role=\"button\"\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\n        <h5 class=\"sidebarBlock-heading\">\n            {{{title}}}\n        </h5>\n\n        <div class=\"accordion-navigation-actions\">\n            {{#if selected}}\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\n            {{/if}}\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use xlink:href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use xlink:href=\"#icon-remove\" />\n            </svg>\n        </div>\n    </div>\n\n    <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"sidebarBlock-content accordion-content facetedSearch-content--{{hyphenate facet}} is-open\">\n        <ul class=\"navList\">\n            {{#each items}}\n                <li class=\"navList-item\">\n                    <a href=\"{{ url }}\"\n                       class=\"navList-action {{#if selected }} is-selected {{/if}}\"\n                       rel=\"nofollow\"\n                       data-faceted-search-facet>\n                        <span class=\"rating--small\">\n                            {{> components/products/ratings rating=title}}\n                        </span>\n                        {{lang 'search.faceted.rating.and-up'}}\n                        {{#if ../show_product_counts}}\n                            <span>({{ count }})</span>\n                        {{/if}}\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n</div>\n{{/if}}\n","components/faceted-search/index":"<div id=\"facetedSearch\" class=\"facetedSearch --{{#if facets.length}}{{#any facets visible=true}}visible{{/any}}{{else}}hidden{{/if}}\">\n    {{> components/faceted-search/selected-facets this.selected}}\n    {{#any facets visible=true}}\n        <div\n            id=\"facetedSearch-navList\"\n            class=\"facetedSearch-navList blocker-container\">\n            <div class=\"accordion accordion--navList\">\n                <p class=\"accordian-title\">\n                    Filter By\n                </p>\n                \n                {{#each facets}}\n                    {{#if visible}}\n                        {{{dynamicComponent 'components/faceted-search/facets'}}}\n                    {{/if}}\n                {{/each}}\n            </div>\n\n            <div class=\"blocker\" style=\"display: none;\"></div>\n        </div>\n    {{/any}}\n</div>\n<script>\n\n\ndocument.addEventListener('DOMContentLoaded', function () {\n    // Check if the body has the class 'page-type-search'\n    if (document.body.classList.contains('page-type-search')) {\n        // Select the elements\n        const facetedSearchContainer = document.getElementById('faceted-search-container');\n        const pageContent = document.querySelector('.page-content');\n        \n        if (facetedSearchContainer) {\n            // Initially hide the faceted search container\n            facetedSearchContainer.style.display = 'none';\n\n            // Set page-content width to 100% when faceted search is hidden\n            if (pageContent) {\n                pageContent.style.width = '100%';\n                pageContent.style.padding = \"0\";\n            }\n        }\n\n        // Start checking for '.navList-item' inside '#faceted-search-container'\n        let timer = setInterval(function () {\n            const navListItem = document.querySelector('#faceted-search-container .navList-item');\n            if (navListItem) {\n                // If found, show the faceted search container\n                facetedSearchContainer.style.display = 'block';\n                \n                // Optionally, reset the page-content width if necessary (or leave it unchanged)\n                if (pageContent) {\n                    pageContent.style.width = ''; // Reset width or set to a specific value\n                }\n\n                clearInterval(timer); // Stop checking\n            }\n        }, 50); // Check every 50ms\n\n        // Stop checking after 2 seconds\n        setTimeout(function () {\n            clearInterval(timer); // Stop the interval after 2 seconds\n        }, 2000);\n    }\n});\n\n\n</script>\n","components/faceted-search/selected-facets":"<div class=\"facetedSearch-refineFilters sidebarBlock comment\">\n    <h5 class=\"sidebarBlock-heading\">\n        {{lang 'search.faceted.selected.title'}}\n    </h5>\n\n    {{#if this.items}}\n        <ul class=\"inlineList inlineList--labels\">\n            {{#each this.items}}\n                <li>\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"facetLabel\"\n                        rel=\"nofollow\"\n                        data-faceted-search-facet>\n                        {{#if facet '===' 'rating'}}\n                            {{lang 'search.faceted.selected.rating-label' rating=title}}\n                        {{else}}\n                            {{{title}}}\n                        {{/if}}\n\n                        <svg class=\"icon\">\n                            <use xlink:href=\"#icon-close\" />\n                        </svg>\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        <div class=\"sidebarBlock-content\">\n            <p>{{lang 'search.faceted.selected.no-filters'}}</p>\n        </div>\n    {{/if}}\n\n    {{#if remove_all_url}}\n        <a href=\"{{ remove_all_url }}\" data-faceted-search-facet>\n            {{lang 'search.faceted.selected.clear-all'}}\n        </a>\n    {{/if}}\n</div>\n","components/products/ratings":"{{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n        <span class=\"icon icon--ratingFull\">\n            <svg>\n                <use xlink:href=\"#icon-star\"/>\n            </svg>\n        </span>\n    {{else}}\n        <span class=\"icon icon--ratingEmpty\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{/if}}\n{{/for}}\n{{{snippet 'product_rating'}}}\n"}
